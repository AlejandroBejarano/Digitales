S_0x55e94c815060 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x55e94c821d30 .scope module, "tb_pc" "tb_pc" 3 3;
 .timescale -9 -12;
P_0x55e94c821ec0 .param/l "N" 0 3 4, +C4<00000000000000000000000000100000>;
v0x55e94c83f8c0_0 .var "StallF", 0 0;
v0x55e94c83f980_0 .var "clk", 0 0;
v0x55e94c83fa20_0 .var "pc_in", 31 0;
v0x55e94c83fb20_0 .net "pc_out", 31 0, v0x55e94c812190_0;  1 drivers
