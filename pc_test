S_0x55e94c821f60 .scope module, "uut" "pc" 3 14, 4 1 0, S_0x55e94c821d30;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "StallF";
    .port_info 3 /INPUT 32 "pc_in";
    .port_info 4 /OUTPUT 32 "pc_out";
P_0x55e94c822140 .param/l "N" 0 4 2, +C4<00000000000000000000000000100000>;
v0x55e94c80eac0_0 .net "StallF", 0 0, v0x55e94c83f8c0_0;  1 drivers
v0x55e94c8103b0_0 .net "clk", 0 0, v0x55e94c83f980_0;  1 drivers
v0x55e94c810d00_0 .net "pc_in", 31 0, v0x55e94c83fa20_0;  1 drivers
v0x55e94c812190_0 .var "pc_out", 31 0;
v0x55e94c83f710_0 .net "reset", 0 0, v0x55e94c83fbf0_0;  1 drivers
E_0x55e94c820470 .event posedge, v0x55e94c83f710_0, v0x55e94c8103b0_0;
    .scope S_0x55e94c821f60;
T_0 ;
    %wait E_0x55e94c820470;
    %load/vec4 v0x55e94c83f710_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x55e94c812190_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55e94c80eac0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x55e94c810d00_0;
    %assign/vec4 v0x55e94c812190_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55e94c821d30;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e94c83f980_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x55e94c821d30;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x55e94c83f980_0;
    %inv;
    %store/vec4 v0x55e94c83f980_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55e94c821d30;
T_3 ;
    %vpi_call/w 3 28 "$dumpfile", "pc_tb.vcd" {0 0 0};
    %vpi_call/w 3 29 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55e94c821d30 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e94c83fbf0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e94c83f8c0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55e94c83fa20_0, 0, 32;
    %pushi/vec4 2, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x55e94c7e7370;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e94c83fbf0_0, 0, 1;
    %vpi_call/w 3 39 "$display", "%0t: Reset desactivado", $time {0 0 0};
    %pushi/vec4 16, 0, 32;
    %store/vec4 v0x55e94c83fa20_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e94c83f8c0_0, 0, 1;
    %wait E_0x55e94c7e7370;
    %delay 1000, 0;
    %vpi_call/w 3 46 "$display", "%0t: pc_in=0x%08h, pc_out=0x%08h (esperado igual)", $time, v0x55e94c83fa20_0, v0x55e94c83fb20_0 {0 0 0};
    %load/vec4 v0x55e94c83fb20_0;
    %load/vec4 v0x55e94c83fa20_0;
    %cmp/ne;
    %jmp/0xz  T_3.2, 6;
    %vpi_call/w 3 47 "$error", "ERROR: pc_out no siguio a pc_in cuando StallF=0" {0 0 0};
T_3.2 ;
    %pushi/vec4 32, 0, 32;
    %store/vec4 v0x55e94c83fa20_0, 0, 32;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e94c83f8c0_0, 0, 1;
    %wait E_0x55e94c7e7370;
    %delay 1000, 0;
    %vpi_call/w 3 54 "$display", "%0t: Stall activo, pc_in=0x%08h, pc_out=0x%08h (esperado sin cambio)", $time, v0x55e94c83fa20_0, v0x55e94c83fb20_0 {0 0 0};
    %load/vec4 v0x55e94c83fb20_0;
    %cmpi/e 32, 0, 32;
    %jmp/0xz  T_3.4, 6;
    %vpi_call/w 3 55 "$error", "ERROR: pc_out cambi\303\263 durante StallF=1" {0 0 0};
T_3.4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e94c83f8c0_0, 0, 1;
    %wait E_0x55e94c7e7370;
    %delay 1000, 0;
    %vpi_call/w 3 61 "$display", "%0t: Stall liberado, pc_in=0x%08h, pc_out=0x%08h (esperado igual)", $time, v0x55e94c83fa20_0, v0x55e94c83fb20_0 {0 0 0};
    %load/vec4 v0x55e94c83fb20_0;
    %load/vec4 v0x55e94c83fa20_0;
    %cmp/ne;
    %jmp/0xz  T_3.6, 6;
    %vpi_call/w 3 62 "$error", "ERROR: pc_out no actualiz\303\263 despu\303\251s de liberar StallF" {0 0 0};
T_3.6 ;
    %delay 3000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e94c83fbf0_0, 0, 1;
    %wait E_0x55e94c7e7370;
    %delay 1000, 0;
    %vpi_call/w 3 69 "$display", "%0t: Reset activado, pc_out=0x%08h (esperado 0)", $time, v0x55e94c83fb20_0 {0 0 0};
    %load/vec4 v0x55e94c83fb20_0;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_3.8, 6;
    %vpi_call/w 3 70 "$error", "ERROR: pc_out no se puso a 0 tras reset" {0 0 0};
T_3.8 ;
    %delay 5000, 0;
    %vpi_call/w 3 74 "$display", "Simulaci\303\263n finalizada correctamente." {0 0 0};
    %vpi_call/w 3 75 "$finish" {0 0 0};
    %end;
    .thread T_3;
